{
  "name": "sst-nextjs-template",
  "version": "0.0.1-beta.0",
  "description": "nextjs sst template project",
  "scripts": {
    "cli": "node --experimental-modules ./scripts/run.mjs",
    "cli:dev": "node --experimental-modules ./scripts/run.mjs -e dev",
    "cli:prod": "node --experimental-modules ./scripts/run.mjs -e prod",
    "cli:staging": "node --experimental-modules ./scripts/run.mjs -e staging",
    "dev": "run-s cli dev:*",
    "dev:start": "next",
    "build": "next build",
    "export": "next export",
    "fix": "run-p fix:*",
    "fix:types": "tsc --noEmit",
    "fix:eslint-fix": "next lint --fix",
    "fix:prettier-fix": "prettier --write '**/*.{js,jsx,md,ts,tsx}'",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci",
    "test": "echo \"Error: no test specified\" && exit 1",
    "e2e:list": "playwright test --reporter=list",
    "e2e:html": "playwright test --reporter=html",
    "e2e:report": "playwright show-report",
    "prisma:dev": "prisma migrate dev"
  },
  "keywords": [
    "nextjs",
    "sst",
    "template"
  ],
  "author": "",
  "license": "ISC",
  "browserslist": [
    "extends @anansi/browserslist-config"
  ],
  "dependencies": {
    "@hookform/resolvers": "^2.9.11",
    "@next/font": "^13.1.5",
    "@prisma/client": "^4.9.0",
    "@sentry/browser": "^7.8.1",
    "@sentry/tracing": "^7.8.1",
    "@tanstack/react-query": "^4.22.0",
    "@tanstack/react-query-devtools": "^4.22.0",
    "body-scroll-lock": "^4.0.0-beta.0",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.7",
    "framer-motion": "^9.0.2",
    "lodash-es": "^4.17.21",
    "next": "^13.1.5",
    "next-seo": "^5.15.0",
    "qs": "^6.11.0",
    "rc-drawer": "^6.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.1",
    "react-icons": "^4.7.1",
    "react-use": "^17.4.0",
    "sass": "^1.57.1",
    "swiper": "^9.0.1",
    "zod": "^3.20.2",
    "zustand": "^4.3.2"
  },
  "devDependencies": {
    "@anansi/browserslist-config": "^1.4.1",
    "@playwright/test": "^1.29.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.18",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "autoprefixer": "^10.4.13",
    "commander": "^10.0.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.32.0",
    "eslint-config-next": "^13.1.5",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "jest": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "prisma": "^4.9.0",
    "shelljs": "^0.8.5",
    "tailwindcss": "^3.2.4",
    "tailwindcss-rtl": "^0.9.0",
    "typescript": "^4.9.4"
  }
}
